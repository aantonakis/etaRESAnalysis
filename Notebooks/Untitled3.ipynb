   "source": [
    "import numpy as np\n",
    "import math\n",
    "\n",
    "def error_factor(N, e):\n",
    "    a = (1 + e**2)*(N-1)\n",
    "    b = 2*e*(N+1)\n",
    "    return a - b\n",
    "\n",
    "\n",
    "print(error_factor(10, 0.2))\n",
    "\n"
   ]
  },
  {
